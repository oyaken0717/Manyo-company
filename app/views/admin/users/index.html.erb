  <table>
    <tr>
      <th>名前</th>
      <th>メール</th>
      <th>admin</th>
      <th>作成日</th>
      <th>更新日</th>
    </tr>
  <% @user.each do |user| %>
    <tr>
      <td><%= user.name%></td>
      <td><%= user.email %></td>
      <td><%= user.admin? ? 'あり' : 'なし' %></td>
      <td><%= link_to "編集", edit_admin_user_path(user.id), class: "btn btn-primary"%></td>
      <td><%= link_to "削除", [:admin, user], method: :delete, data: {confirm: "本当に削除してもいいですか？"}, class: "btn btn-primary" %></td>
    </tr>
  <% end %>
