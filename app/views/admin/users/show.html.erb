<table>
  <tr>
    <th>仕事名</th>
    <th>仕事内容</th>
    <th>終了期限</th>
    <th>進捗</th>
    <th>順位</th>
  </tr>
<% @user.tasks.each do |user_task| %>
    <tr>
      <td><%= user_task.title%></td>
      <td><%= user_task.content %></td>
      <td><%= user_task.deadline %></td>
      <td><%= user_task.status %></td>
      <td><%= user_task.priority %></td>
    </tr>
<% end %>
</table>
